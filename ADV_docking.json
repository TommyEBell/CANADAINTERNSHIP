{
  "docking": {
    "header": {
      "logging": {
        "logfile": "/content/ADV_docking.log"
      }
    },
    "ligand_preparation": {
      "embedding_pools": [
        {
          "pool_id": "RDkit_pool",
          "type": "RDkit",
          "input": {
            "standardize_smiles": false
          }
        }
      ]
    },
    "docking_runs": [
      {
        "backend": "AutoDockVina",
        "run_id": "AutoDockVina",
        "input_pools": [
          "RDkit_pool"
        ],
        "parameters": {
          "binary_location": "/usr/local/miniconda/bin/",
          "parallelization": {
            "number_cores": 2
          },
          "seed": 42,
          "receptor_pdbqt_path": [
            "/content/ADV_receptor.pdbqt"
          ],
          "number_poses": 2,
          "search_space": {
            "--center_x": -39.7,
            "--center_y": 17,
            "--center_z": 12.6,
            "--size_x": 15,
            "--size_y": 10,
            "--size_z": 10
          }
        },
        "output": {
          "poses": {
            "poses_path": "/content/ADV_ligands_docked.sdf",
            "overwrite": false
          },
          "scores": {
            "scores_path": "/content/ADV_scores.csv",
            "overwrite": false
          }
        }
      }
    ]
  }
}